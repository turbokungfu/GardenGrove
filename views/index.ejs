<%- include('partials/header') -%>
  <body>
    <div class="container">

        <header class="d-flex justify-content-center py-3">
          <ul class="nav nav-pills ">
            <li class="nav-item"><a href="/about" class="nav-link">About</a></li>
            <li class="nav-item"><a href="/login" class="nav-link">Login</a></li>
            <li class="nav-item"><a href="/signup" class="nav-link">Sign up</a></li>
          </ul>

        </header>
        <div class="container col-12 col-sm-12 col-md-12 col-lg-6 float-start">
          <div
            id="map"

          ></div>
        </div>
        <div class="container col-12 col-sm-12 col-md-12 col-lg-6 float-end">
          <ul class="row list-unstyled">
            <li class="box">
               <% for(var i=0; i<3; i++){ %>
                 <a href="/signup">
                   
                   <img class="img-fluid pic" src="<%= posts[i].image%>"> 
                 </a>
       
       
               <div class="side"><%= posts[i].location.formattedAddress%>
               <%= posts[i].title%>
               <%= posts[i].caption%></div>
                   <%}%>
       
                 </li>
           </ul>
        </div>
      </div>




      
  </body>
  <script src="https://api.mapbox.com/mapbox-gl-js/v1.4.1/mapbox-gl.js"></script>
<script src="js/map.js"></script>
<script src="js/add.js"></script>

